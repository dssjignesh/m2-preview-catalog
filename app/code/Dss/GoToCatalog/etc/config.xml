<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <dssgotocatalog>
            <general>
                <enabled>1</enabled>
                <debug>0</debug>
            </general>
            <catalog>
                <enable_product_link>1</enable_product_link>
                <enable_category_link>1</enable_category_link>
                <product_url_key>goto</product_url_key>
            </catalog>
        </dssgotocatalog>
    </default>
</config>
